{
  "formId": "eRecruiter_form",
  "formName": "eRecruiter default form",
  "fields": [
    {
      "fieldName": "FirstName",
      "selector": "//input[contains(@name, 'tbName')]",
      "dataValue": "${FirstName}",
      "fieldType": "text"
    },
    {
      "fieldName": "LastName",
      "selector": "//input[contains(@name, 'tbSurname')]",
      "dataValue": "${LastName}",
      "fieldType": "text"
    },
    {
      "fieldName": "Mail",
      "selector": "//input[contains(@name, 'tbMail')]",
      "dataValue": "${Email}",
      "fieldType": "text"
    },
    {
      "fieldName": "Phone",
